{
  "kind": "collectionType",
  "collectionName": "article_tags",
  "info": {
    "singularName": "tag",
    "pluralName": "tags",
    "displayName": "Article tag"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "slug": {
      "type": "customField",
      "customField": "plugin::pinyin-slug.pinyin-slug",
      "options": {
        "sourceField": "name"
      },
      "required": true,
      "unique": true
    },
    "topArticles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article.article"
    },
    "articles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::article.article",
      "mappedBy": "tags"
    },
    "seo": {
      "type": "component",
      "component": "shared.seo",
      "repeatable": false
    }
  }
}
